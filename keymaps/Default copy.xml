<keymap version="1" name="Default copy" parent="$default">
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="f9" />
  </action>
  <action id="VimPluginToggle">
    <keyboard-shortcut first-keystroke="ctrl alt v" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
</keymap>